<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home"
    tools:ignore="UnusedNavigation">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.dailybucket.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.dailybucket.UserProfileFragment"
        android:label="Profile"
        tools:layout="@layout/userprofile"/>
    <fragment
        android:id="@+id/navigation_search"
        android:name="com.example.dailybucket.SearchFragment"
        android:label="@string/search"
        tools:layout="@layout/search"/>

</navigation>